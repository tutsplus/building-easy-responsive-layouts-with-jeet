@import '../../../bower_components/jeet/stylus/jeet'

*
  margin: 0
  padding: 0
  font-family: sans-serif
  box-sizing: border-box

edit(blue)

.container
  center(800px)

.logo
  span(1/5)

nav
  span(3/5, offset: 1/5)

.gallery
  center(800px)
  img
    stack()
    @media (min-width: 500px)
      span(1/2, cycle: 2)
    @media (min-width: 800px)
      span(1/3, uncycle: 2, cycle: 3)

